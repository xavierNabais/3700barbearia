<head>

</head>
<body>

    <form method="POST">
        <table>
            <tr>
                <th>Id</th>
                <td><input type="text" name="id" value="<%= dados[0].Id %>"></td>
            </tr>
            <tr>
                <th>Nome</th>
                <td><input type="text" name="nome" value="<%= dados[0].Nome %>"></td>
            </tr>
            <tr>
                <th>Preço</th>
                <td><input type="number" name="preco" value="<%= dados[0].Preco %>"></td>
            </tr>
            <tr>
                <th>Duração</th>
                <td><input type="number" name="duracao" value="<%= dados[0].Duracao %>"></td>
            </tr>
            <tr>
                <th>Ativo</th>
                <td><input type="number" name="ativo" value="<%= dados[0].Ativo %>"></td>
            </tr>
        </table>
        <input type="submit" value="Enviar">
    </form>
    

</body>

</html>
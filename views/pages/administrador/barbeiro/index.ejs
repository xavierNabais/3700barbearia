<!DOCTYPE html>
<html lang="pt">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Agendamento de Barbearia</title>
    <link rel="stylesheet" type="text/css" href="../../assets/css/3700barber.css">
</head>
<body>

    <a href="/painel/barbeiros/novo" class="btn btn-primary btn-xs"><b>+</b> Adicionar novo barbeiro</a>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Descrição</th>
                <th>Especialização</th>
                <th>Ativo</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            <% dados.forEach((dados) => { %>
            <tr>
                <td><%= dados.Id %></td>
                <td><%= dados.Nome %></td>
                <td><%= dados.Descricao %></td>
                <td><%= dados.Especializacao %></td>
                <td><%= dados.Ativo %></td>
                <td><a href="/painel/barbeiros/update/<%= dados.Id %>">Editar</a>|<a href="/painel/barbeiros/delete/<%= dados.Id %>">Apagar</a></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
    


</body>
</html>
